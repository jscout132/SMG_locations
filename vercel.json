{
    "version": 2,
    "builds": [
        {
            "src": "*.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "(.*)",
            "dest": "app.py"
        }
    ],
    "functions": {
        "static/*.js": {
            "memory": 1024,
            "maxDuration": 10
        }
    }
  }